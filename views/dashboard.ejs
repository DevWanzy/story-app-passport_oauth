<h6>Dashboard</h6>
<h3>Welcome <%= name %></h3>
<p>Here are your stories</p>

<% if(stories){ %>

<table class="striped">
    <thead>
        <tr>
            <th>Title</th>
            <th>Date</th>
            <th>Status</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% for( let story of stories){%>
        <tr>
            <td><a href="/stories/<%=id%>"><%= title %></a></td>
        </tr>
        <%}%>
    </tbody>
</table>
<% } else { %>
<p>you have not created any story</p>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Date</th>
            <th>Status</th>
            <th></th>
        </tr>
    </thead>
</table>
<% } %>
